  "views": {
    "core.identity_credentials": {
      "columns": {
        "id": {
          "name": "id",
          "type": "uuid",
          "primaryKey": true,
          "notNull": true
        },
        "passwordHash": {
          "name": "passwordHash",
          "type": "varchar(128)",
          "primaryKey": false,
          "notNull": true
        },
        "phoneNumber": {
          "name": "phoneNumber",
          "type": "varchar(15)",
          "primaryKey": false,
          "notNull": true
        },
        "email": {
          "name": "email",
          "type": "varchar(128)",
          "primaryKey": false,
          "notNull": true
        }
      },
      "definition": "\n  SELECT\n    \"core\".\"identity\".\"id\",\n    \"lastPassword\".\"value\" AS \"passwordHash\",\n    \"lastPhoneNumber\".\"value\" AS \"phoneNumber\",\n    \"lastEmail\".\"value\" AS \"email\"\n  FROM \"core\".\"identity\"\n  \n  LEFT JOIN LATERAL (\n    SELECT \"value\", \"identityId\" FROM \"core\".\"password\"\n    WHERE \"core\".\"password\".\"identityId\" = \"core\".\"identity\".\"id\"\n    ORDER BY \"core\".\"password\".\"createdAt\" DESC LIMIT 1\n  ) \"lastPassword\" ON \"lastPassword\".\"identityId\" = \"core\".\"identity\".\"id\"\n\n  LEFT JOIN LATERAL (\n    SELECT \"value\", \"identityId\" FROM \"core\".\"phone_number\"\n    WHERE \"core\".\"phone_number\".\"identityId\" = \"core\".\"identity\".\"id\"\n    ORDER BY \"core\".\"phone_number\".\"createdAt\" DESC LIMIT 1\n  ) \"lastPhoneNumber\" ON \"lastPhoneNumber\".\"identityId\" = \"core\".\"identity\".\"id\"\n\n  LEFT JOIN LATERAL (\n    SELECT \"value\", \"identityId\" FROM \"core\".\"email\"\n    WHERE \"core\".\"email\".\"identityId\" = \"core\".\"identity\".\"id\"\n    ORDER BY \"core\".\"email\".\"createdAt\" DESC LIMIT 1\n  ) \"lastEmail\" ON \"lastEmail\".\"identityId\" = \"core\".\"identity\".\"id\"\n\n  WHERE \"core\".\"identity\".\"removedAt\" IS NULL\n",
      "name": "identity_credentials",
      "schema": "core",
      "isExisting": false,
      "materialized": false
    }
