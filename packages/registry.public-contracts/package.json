{
  "name": "@omg/public-contracts-registry",
  "private": true,
  "version": "0.0.0",
  "description": "Collection of all endpoints and associated schemas/instructions",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./lib/index.ts",
  "exports": {
    ".": {
      "types": "./lib/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./lib/*.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "start:dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "utils",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "cli-color": "^2.0.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@omg/eslint-config": "workspace:*",
    "@types/cli-color": "^2.0.6",
    "eslint": "^9.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "drizzle-orm": "*"
  }
}
